___SANDBOXED_JS_FOR_WEB_TEMPLATE___

var log = require('logToConsole');
var getCookieValues = require('getCookieValues');
var setCookie = require('setCookie');
var getUrl = require('getUrl');
var injectScript = require('injectScript');
var queryPermission = require('queryPermission');

var testCode = data.testCode;
var testVersion = data.testVersion;
var inclusionRate = data.inclusionRate * 1;
var cookieDays = 7;
var cookieKey = testCode + '_' + testVersion;

function readCookieValue(cookieName) {
  if (queryPermission('get_cookies', cookieName)) {
    var values = getCookieValues(cookieName);
    return values.length > 0 ? values[0] : null;
  } else {
    log('Permission denied for reading cookie: ' + cookieName);
    return null;
  }
}

function getQueryParam(key) {
  var search = getUrl().search || '';
  if (search.charAt(0) === '?') {
    search = search.substring(1);
  }
  var query = search.split('&');
  for (var i = 0; i < query.length; i++) {
    var parts = query[i].split('=');
    if (parts[0] === key) {
      return parts[1] || '';
    }
  }
  return null;
}

function runVariant(variantName, variantCode) {
  if (!variantCode || typeof variantCode !== 'string') {
    log(testCode + ': No variant code defined for ' + variantName);
    data.gtmOnSuccess();
    return;
  }

  log(testCode + ': Running ' + variantName);
  data.variantAssigned = variantName;

  setCookie(cookieKey, variantName, {
    'max-age': cookieDays * 86400
  });

  injectScript(variantCode, data.gtmOnSuccess, function (err) {
    log('Error injecting variant code: ' + err);
    data.gtmOnSuccess();
  });

  log('Code on this page was developed by www.netimpression.co.uk 🚀');
}

var forced = getQueryParam('mvt');
var existing = readCookieValue(cookieKey);

// 1. Forced via ?mvt=
if (forced) {
  var forcedCode = null;
  if (forced === 'control') forcedCode = data.controlCode;
  else if (forced === 'variation1') forcedCode = data.variation1Code;
  else if (forced === 'variation2') forcedCode = data.variation2Code;
  else if (forced === 'variation3') forcedCode = data.variation3Code;

  if (forcedCode) runVariant(forced, forcedCode);
  else data.gtmOnSuccess();
}

// 2. Already assigned via cookie
else if (existing) {
  var variantCode = null;
  if (existing === 'control') variantCode = data.controlCode;
  else if (existing === 'variation1') variantCode = data.variation1Code;
  else if (existing === 'variation2') variantCode = data.variation2Code;
  else if (existing === 'variation3') variantCode = data.variation3Code;

  if (variantCode) runVariant(existing, variantCode);
  else data.gtmOnSuccess();
}

// 3. Fresh assignment
else {
  testCode = '' + testCode;

  var allowed = 'abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789';
  var seed = 0;

  for (var i = 0; i < testCode.length; i++) {
    var ch = testCode.charAt(i);
    var idx = allowed.indexOf(ch);
    if (idx !== -1) {
      seed += idx;
    }
  }

  if (data.gtmTagId && typeof data.gtmTagId.length === 'number') {
    seed += data.gtmTagId.length;
  }

  var rand = (seed % 1000) / 1000;

  if (rand < inclusionRate) {
    var variantCount = data.variantCount * 1;
    if (!variantCount || variantCount < 1 || variantCount > 3) variantCount = 3;

    var index = seed % (variantCount + 1);
    var variantName = index === 0 ? 'control' : 'variation' + index;
    var code = null;

    if (variantName === 'control') code = data.controlCode;
    else if (variantName === 'variation1') code = data.variation1Code;
    else if (variantName === 'variation2') code = data.variation2Code;
    else if (variantName === 'variation3') code = data.variation3Code;

    runVariant(variantName, code);
  } else {
    log(testCode + ': User not included in test');
    data.gtmOnSuccess();
  }
}


